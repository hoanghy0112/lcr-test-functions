    "dotenv": "^16.4.7",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.3"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}