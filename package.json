{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "",
    "deploy": "gcloud functions deploy FUNCTION_NAME --region=us-central1 --runtime=nodejs22 --entry-point=uploadFile --trigger-http --allow-unauthenticated --memory=1gb --cpu=1 --max-instances=20 --concurrency=5 --timeout=3600s",
